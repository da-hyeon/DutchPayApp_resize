<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#f3f1f2">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="24.75dp"
                android:fontFamily="@font/malgunfont"
                android:includeFontPadding="false"
                android:text="@string/register_titleText"
                android:textColor="#2a2a2a"
                android:textSize="18.85dp"
                android:textStyle="bold"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <LinearLayout
                android:id="@+id/linearLayout"
                android:layout_width="match_parent"
                android:layout_height="286.85dp"
                android:layout_marginLeft="17.14dp"
                android:layout_marginTop="14.50dp"
                android:layout_marginRight="17.42dp"
                android:background="#ffffff"
                android:orientation="horizontal"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView1" />

            <CheckBox
                android:id="@+id/cbCompleteAgreement"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="21.25dp"
                android:button="@drawable/myallcheck"
                app:layout_constraintBottom_toTopOf="@+id/view"
                app:layout_constraintLeft_toLeftOf="@+id/linearLayout"
                app:layout_constraintTop_toTopOf="@+id/linearLayout" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10.25dp"
                android:fontFamily="@font/malgunfont"
                android:includeFontPadding="false"
                android:text="@string/register_agreeTitleText"
                android:textColor="#1e1e1e"
                android:textSize="14dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/cbCompleteAgreement"
                app:layout_constraintLeft_toRightOf="@+id/cbCompleteAgreement"
                app:layout_constraintTop_toTopOf="@+id/cbCompleteAgreement" />

            <!---->

            <View
                android:id="@+id/view"
                android:layout_width="0dp"
                android:layout_height="0.25dp"
                android:layout_centerInParent="true"
                android:layout_marginTop="52.28dp"
                android:background="#cdcdcd"
                app:layout_constraintLeft_toLeftOf="@+id/linearLayout"
                app:layout_constraintRight_toRightOf="@+id/linearLayout"
                app:layout_constraintTop_toTopOf="@+id/linearLayout" />

            <!---->

            <ImageView
                android:id="@+id/check1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15.25dp"
                android:src="@drawable/check_box"
                app:layout_constraintLeft_toLeftOf="@+id/cbCompleteAgreement"
                app:layout_constraintTop_toBottomOf="@+id/view" />

            <CheckBox
                android:id="@+id/cbTermsConditions1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/mycheck"
                app:layout_constraintLeft_toLeftOf="@+id/check1"
                app:layout_constraintTop_toTopOf="@+id/check1" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10.25dp"
                android:fontFamily="@font/malgunfont"
                android:includeFontPadding="false"
                android:text="@string/register_agree1Text"

                android:textColor="#1e1e1e"
                android:textSize="13.71dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/cbTermsConditions1"
                app:layout_constraintLeft_toRightOf="@+id/check1"
                app:layout_constraintTop_toTopOf="@+id/cbTermsConditions1" />

            <ImageView
                android:id="@+id/viewAll_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="21.25dp"
                android:src="@drawable/view_all_off"
                app:layout_constraintBottom_toBottomOf="@+id/cbTermsConditions1"
                app:layout_constraintRight_toRightOf="@+id/linearLayout"
                app:layout_constraintTop_toTopOf="@+id/cbTermsConditions1" />

            <!---->
            <ImageView
                android:id="@+id/check2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:src="@drawable/check_box"
                app:layout_constraintLeft_toLeftOf="@+id/cbCompleteAgreement"
                app:layout_constraintTop_toBottomOf="@+id/check1" />


            <CheckBox
                android:id="@+id/cbTermsConditions2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/mycheck"
                app:layout_constraintLeft_toLeftOf="@+id/check2"
                app:layout_constraintTop_toTopOf="@+id/check2" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/malgunfont"
                android:text="@string/register_agree2Text"
                android:textColor="#1e1e1e"
                android:textSize="13.71dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/cbTermsConditions2"
                app:layout_constraintLeft_toLeftOf="@+id/textView5"
                app:layout_constraintTop_toTopOf="@+id/cbTermsConditions2" />

            <ImageView
                android:id="@+id/viewAll_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/view_all_off"
                app:layout_constraintBottom_toBottomOf="@+id/cbTermsConditions2"
                app:layout_constraintRight_toRightOf="@+id/viewAll_1"
                app:layout_constraintTop_toTopOf="@+id/cbTermsConditions2" />

            <!---->
            <ImageView
                android:id="@+id/check3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:src="@drawable/check_box"
                app:layout_constraintLeft_toLeftOf="@+id/cbCompleteAgreement"
                app:layout_constraintTop_toBottomOf="@+id/check2" />


            <CheckBox
                android:id="@+id/cbTermsConditions3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/mycheck"
                app:layout_constraintLeft_toLeftOf="@+id/check3"
                app:layout_constraintTop_toTopOf="@+id/check3" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/malgunfont"
                android:text="@string/register_agree3Text"
                android:textColor="#1e1e1e"
                android:textSize="13.71dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/cbTermsConditions3"
                app:layout_constraintLeft_toLeftOf="@+id/textView6"
                app:layout_constraintTop_toTopOf="@+id/cbTermsConditions3" />

            <ImageView
                android:id="@+id/viewAll_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/view_all_off"
                app:layout_constraintBottom_toBottomOf="@+id/cbTermsConditions3"
                app:layout_constraintRight_toRightOf="@+id/viewAll_1"
                app:layout_constraintTop_toTopOf="@+id/cbTermsConditions3" />


            <!---->
            <ImageView
                android:id="@+id/check4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:src="@drawable/check_box"
                app:layout_constraintLeft_toLeftOf="@+id/cbCompleteAgreement"
                app:layout_constraintTop_toBottomOf="@+id/check3" />


            <CheckBox
                android:id="@+id/cbTermsConditions4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/mycheck"
                app:layout_constraintLeft_toLeftOf="@+id/check4"
                app:layout_constraintTop_toTopOf="@+id/check4" />


            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/malgunfont"
                android:text="@string/register_agree4Text"
                android:textColor="#1e1e1e"
                android:textSize="13.71dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/cbTermsConditions4"
                app:layout_constraintLeft_toLeftOf="@+id/textView7"
                app:layout_constraintTop_toTopOf="@+id/cbTermsConditions4" />

            <ImageView
                android:id="@+id/viewAll_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/view_all_off"
                app:layout_constraintBottom_toBottomOf="@+id/cbTermsConditions4"
                app:layout_constraintRight_toRightOf="@+id/viewAll_1"
                app:layout_constraintTop_toTopOf="@+id/cbTermsConditions4" />


            <!---->
            <ImageView
                android:id="@+id/check5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:src="@drawable/check_box"
                app:layout_constraintLeft_toLeftOf="@+id/cbCompleteAgreement"
                app:layout_constraintTop_toBottomOf="@+id/check4" />


            <CheckBox
                android:id="@+id/cbTermsConditions5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/mycheck"
                app:layout_constraintLeft_toLeftOf="@+id/check5"
                app:layout_constraintTop_toTopOf="@+id/check5" />


            <TextView
                android:id="@+id/textVie9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/malgunfont"
                android:text="@string/register_agree5Text"
                android:textColor="#1e1e1e"
                android:textSize="13.71dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/cbTermsConditions5"
                app:layout_constraintLeft_toLeftOf="@+id/textView8"
                app:layout_constraintTop_toTopOf="@+id/cbTermsConditions5" />


            <ImageView
                android:id="@+id/viewAll_5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/view_all_off"
                app:layout_constraintBottom_toBottomOf="@+id/cbTermsConditions5"
                app:layout_constraintRight_toRightOf="@+id/viewAll_1"
                app:layout_constraintTop_toTopOf="@+id/cbTermsConditions5" />


            <!---->
            <ImageView
                android:id="@+id/check6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:src="@drawable/check_box"
                app:layout_constraintLeft_toLeftOf="@+id/cbCompleteAgreement"
                app:layout_constraintTop_toBottomOf="@+id/check5" />


            <CheckBox
                android:id="@+id/cbTermsConditions6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/mycheck"
                app:layout_constraintLeft_toLeftOf="@+id/check6"
                app:layout_constraintTop_toTopOf="@+id/check6" />

            <TextView
                android:id="@+id/textVie10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/malgunfont"
                android:text="@string/register_agree6Text"
                android:textColor="#1e1e1e"
                android:textSize="13.71dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/cbTermsConditions6"
                app:layout_constraintLeft_toLeftOf="@+id/textVie9"
                app:layout_constraintTop_toTopOf="@+id/cbTermsConditions6" />

            <ImageView
                android:id="@+id/viewAll_6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:fontFamily="@font/malgunfont"
                android:src="@drawable/view_all_off"
                app:layout_constraintBottom_toBottomOf="@+id/cbTermsConditions6"
                app:layout_constraintRight_toRightOf="@+id/viewAll_1"
                app:layout_constraintTop_toTopOf="@+id/cbTermsConditions6" />


            <!---->

            <ImageView
                android:id="@+id/imageView_register"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="26dp"
                android:scaleType="fitXY"
                android:src="@drawable/join_btn"
                app:layout_constraintLeft_toLeftOf="@+id/linearLayout"
                app:layout_constraintRight_toRightOf="@+id/linearLayout"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

            <ImageView
                android:id="@+id/imageView_kakaogo"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="9.7dp"
                android:scaleType="fitXY"
                android:src="@drawable/join_join_kakao"
                app:layout_constraintLeft_toLeftOf="@+id/linearLayout"
                app:layout_constraintRight_toRightOf="@+id/linearLayout"
                app:layout_constraintTop_toBottomOf="@+id/imageView_register" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="9.50dp"
                android:fontFamily="@font/malgunfont"
                android:text="@string/register_agree7Text"
                android:textSize="9.7dp"
                android:textStyle="bold"
                app:layout_constraintLeft_toLeftOf="@+id/linearLayout"
                app:layout_constraintTop_toBottomOf="@+id/imageView_kakaogo" />

        </android.support.constraint.ConstraintLayout>
    </android.support.constraint.ConstraintLayout>
</layout>